		<Archiver>
			<OpeningMode value="RECREATE" />
			<ArchiveName value="ExampleModuleArchive" />
			<ArchiverDirectory   value="/tmp"/>
		</Archiver>
		
		<Module name="MyExampleModule" type="ExampleModule">
		   <ConfigFile> /home/acqilc/dqm_setup/m3_avril2015.xml</ConfigFile>
		</Module>
	
	</ModuleApplication>
	
</dqm4hep>
