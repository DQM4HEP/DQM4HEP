<dqm4hep>

  <ModuleApplication archiver="on">
  
    <Network>
      <RunControl           value="SDHCALRunControl" />
      <DataCollectorName              value="RawbinDatacollector" />
      <EventStreamerType                 value="LCIOStreamer"/>
      <MonitorElementCollectorName       value="MEcollector"/>
    </Network>
		
		<Cycle>
			<CycleType    value="TimerCycle" />
			<CycleValue   value="10" />
			<CycleTimeout value="5" />
