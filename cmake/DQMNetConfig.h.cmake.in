#ifndef DQMNetConfig_H
#define DQMNetConfig_H

// version macros
#define DQMNet_MAJOR_VERSION @DQMNet_VERSION_MAJOR@
#define DQMNet_MINOR_VERSION @DQMNet_VERSION_MINOR@
#define DQMNet_PATCH_LEVEL @DQMNet_VERSION_PATCH@

#define DQMNet_VERSION_STR "@DQMNet_VERSION_MAJOR@.@DQMNet_VERSION_MINOR@.@DQMNet_VERSION_PATCH@"

#define DQMNet_VERSION_GE( MAJV , MINV , PLEV )  \
     ( (DQMNet_MAJOR_VERSION > MAJV) || ( (DQMNet_MAJOR_VERSION == MAJV) && (DQMNet_MINOR_VERSION > MINV) ) \
  || ( (DQMNet_MAJOR_VERSION == MAJV) && (DQMNet_MINOR_VERSION == MINV) && (DQMNet_PATCH_LEVEL >= PLEV) ) )

// project source dir
#define DQMNet_DIR "@CMAKE_INSTALL_PREFIX@"

#endif // DQMNetConfig_H
